/bin/rm -r plt* chk* out.i*flow
./incflo2d.gnu.MPI.EB.ex inputs_cyl_left  xhi.velocity=-1.0  0.0  0.0 incflo.ic_u=-1.0 eb.flow_velocity=-1. 0. 0. >& out.iml_coflow
mv plt00010 plt00010_iml_c
./incflo2d.gnu.MPI.EB.ex inputs_cyl_left  xhi.velocity= 0.0  0.0  0.0 incflo.ic_u=0.0 eb.flow_velocity=-1. 0. 0. >& out.iml_noflow
mv plt00010 plt00010_iml_n
./incflo2d.gnu.MPI.EB.ex inputs_cyl_right xlo.velocity= 1.0  0.0  0.0 incflo.ic_u=1.0 eb.flow_velocity=1. 0. 0. >& out.imr_coflow
mv plt00010 plt00010_imr_c
./incflo2d.gnu.MPI.EB.ex inputs_cyl_right xlo.velocity= 0.0  0.0  0.0 incflo.ic_u=0.0 eb.flow_velocity=1. 0. 0. >& out.imr_noflow
mv plt00010 plt00010_imr_n
./incflo2d.gnu.MPI.EB.ex inputs_cyl_down  yhi.velocity= 0.0 -1.0  0.0 incflo.ic_v=-1.0 eb.flow_velocity=0. -1. 0. >& out.imd_coflow
mv plt00010 plt00010_imd_c
./incflo2d.gnu.MPI.EB.ex inputs_cyl_down  yhi.velocity= 0.0  0.0  0.0 incflo.ic_v=0.0 eb.flow_velocity=0. -1. 0. >& out.imd_noflow
mv plt00010 plt00010_imd_n
./incflo2d.gnu.MPI.EB.ex inputs_cyl_up    ylo.velocity= 0.0  1.0  0.0 incflo.ic_v=1.0 eb.flow_velocity=0. 1. 0. >& out.imu_coflow
mv plt00010 plt00010_imu_y
./incflo2d.gnu.MPI.EB.ex inputs_cyl_up    ylo.velocity= 0.0  0.0  0.0 incflo.ic_v=0.0 eb.flow_velocity=0. 1. 0. >& out.imu_noflow
mv plt00010 plt00010_imu_n
echo "IML COFLOW"
grep Initial out.iml_coflow | head -4
echo "IML NOFLOW"
grep Initial out.iml_noflow | head -4
echo "IMR COFLOW"
grep Initial out.imr_coflow | head -4
echo "IMR NOFLOW"
grep Initial out.imr_noflow | head -4
echo "IMD COFLOW"
grep Initial out.imd_coflow | head -4
echo "IMD NOFLOW"
grep Initial out.imd_noflow | head -4
echo "IMU COFLOW"
grep Initial out.imu_coflow | head -4
echo "IMU NOFLOW"
grep Initial out.imu_noflow | head -4
